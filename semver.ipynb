    "\n",
    "import semver\n",
    "\n",
    "with pathlib.Path('version').open('r', encoding='utf-8') as v_fh:\n",
    "    version_info = semver.parse_version_info(v_fh.read())\n",
    "\n",
    "version_info = version_info.bump_major().bump_prerelease()\n",
    "\n",
    "version_info"
